config CPU_INTEL_SOCKET_FC_PGA370
	bool
	select CPU_INTEL_MODEL_68X
	select MMX
	select SSE

if CPU_INTEL_SOCKET_FC_PGA370

config DCACHE_RAM_BASE
	hex
	default 0xc8000

config DCACHE_RAM_SIZE
	hex
	default 0x08000

endif
